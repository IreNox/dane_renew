    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}/../../../../bin/letsencrypt_helper.exe"
        },
        {
            "name": "Debug Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "dane-update",
                "-domain=www.ioniel.de",
                "-cert=www.ioniel.net.pem"
            ]
        }
    ]
}